<apex:page standardController="Student__c" tabStyle="Contact">
    <apex:form >
        <apex:pageblock title="Student Registration">
            <apex:pageblockSection title="Student Details" columns="1">                
                <apex:inputField label="Name" value="{!Student__c.Name}" required="true"/>
                <apex:inputField label="Register Number" value="{!Student__c.Roll_Number__c}"/>
                <apex:inputField label="College Name" value="{!Student__c.College__c}" required="true"/>
                <apex:inputField label="Class Name" value="{!Student__c.Class__c}" required="true"/>
                <apex:inputField label="Gender" value="{!Student__c.Gender__c}"/>
                <apex:inputField label="Date of Birth" value="{!Student__c.D_O_B__c}"/>
                <apex:inputField label="Contact Number" value="{!Student__c.Contact_Number__c}"/>
                <apex:inputField label="Email"  value="{!Student__c.E_Mail__c}"/>
            </apex:pageblockSection>
            <apex:pageblockSection title="Student Marks" columns="1">
                <apex:inputField label="Tamil" value="{!Student__c.Mark_1__c}"/>
                <apex:inputField label="English" value="{!Student__c.Mark_2__c}"/>
                <apex:inputField label="Software Engineering" value="{!Student__c.Mark_3__c}"/>
                <apex:inputField label="Data Mining" value="{!Student__c.Mark_4__c}"/>
                <apex:inputField label="Programming in Java" value="{!Student__c.Mark_5__c}"/>
                <apex:commandButton value="Save" action="{!save}"/>
            </apex:pageblockSection>
        </apex:pageblock>
    </apex:form>
</apex:page>