<apex:page standardController="Student__c" recordSetVar="Student__c">
    <apex:pageBlock title="Students List">
    <apex:pageBlockTable value="{! Student__c}" var="ct">
        <apex:column value="{!ct.Name}"/>
        <apex:column value="{!ct.College__r.Name}"/>
        <apex:column value="{!ct.Class__r.Name}"/>
        <apex:column value="{!ct.Gender__c}"/>
        </apex:pageBlockTable>
    </apex:pageBlock>
</apex:page>